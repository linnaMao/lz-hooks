{
  "private": true,
  "name": "lz-hooks",
  "version": "1.0.0",
  "description": "react hooks library",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "start": "dumi dev",
    "build": "father-build && tsc",
    "test": "umi-test"
